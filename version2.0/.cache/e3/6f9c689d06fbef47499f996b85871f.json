:0},"source":"lib/react-wheel-dom.js","original":{"line":42,"column":1}},{"generated":{"line":52,"column":15},"source":"lib/react-wheel-dom.js","original":{"line":44,"column":15}},{"name":"render","generated":{"line":53,"column":0},"source":"lib/react-wheel-dom.js","original":{"line":44,"column":16}},{"name":"render","generated":{"line":53,"column":2},"source":"lib/react-wheel-dom.js","original":{"line":44,"column":16}},{"generated":{"line":53,"column":8},"source":"lib/react-wheel-dom.js","original":{"line":44,"column":22}},{"name":"render","generated":{"line":53,"column":10},"source":"lib/react-wheel-dom.js","original":{"line":44,"column":16}},{"generated":{"line":54,"column":0},"source":"lib/react-wheel-dom.js","original":{"line":44,"column":15}}],"sources":{"lib/react-wheel-dom.js":"//渲染虚拟dom\n\nfunction render(vnode,container) {\n    // 在每一次渲染之前先清空上一次渲染进去的东西,避免重复\n    container.innerHTML = ''\n    _render(vnode,container)\n}\n\nfunction _render(vnode, container) {\n    //如果最后的叶子节点是一个字符串\n    if (typeof vnode === 'string' || typeof vnode === 'number') {\n        return container.appendChild(document.createTextNode(vnode))\n    }\n\n    if (typeof vnode === 'object') {\n        let dom = document.createElement(vnode.tag)\n        setAttribute(dom, vnode.attrs)\n        if (vnode.children && Array.isArray(vnode.children)) {\n            vnode.children.forEach(vnodeChild => {\n                _render(vnodeChild, dom)\n            })\n        }\n        container.appendChild(dom)\n    }\n\n}\n\nfunction setAttribute(dom, attrs) {\n    for (let key in attrs) {\n        //事件绑定\n        if (/^on/.test(key)) {\n            dom[key.toLocaleLowerCase()] = attrs[key]\n            // 样式\n        } else if (key === 'style') {\n            Object.assign(dom.style, attrs[key])\n        } else {\n            // 属性\n            dom[key] = attrs[key]\n        }\n    }\n\n}\n\nexport default {render}"},"lineCount":null}},"error":null,"hash":"e3cd80d8f641cdba1304ab106225e418","cache